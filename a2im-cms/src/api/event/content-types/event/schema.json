{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Description": {
      "type": "richtext"
    },
    "StartDate": {
      "type": "datetime"
    },
    "EndDate": {
      "type": "datetime"
    },
    "Cover": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "URL": {
      "type": "string"
    },
    "sponsor_packages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sponsor-package.sponsor-package",
      "mappedBy": "event"
    }
  }
}
