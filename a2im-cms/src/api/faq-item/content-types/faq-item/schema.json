{
  "kind": "collectionType",
  "collectionName": "faq_items",
  "info": {
    "singularName": "faq-item",
    "pluralName": "faq-items",
    "displayName": "FAQ item"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "apps": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::app.app",
      "inversedBy": "faq_items"
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "Award Categories & Descriptions",
        "General Award Eligibility Rules",
        "Award Attachments Criteria",
        "Tickets",
        "Venue",
        "Voting"
      ],
      "required": true
    },
    "Question": {
      "type": "string",
      "required": true
    },
    "Answer": {
      "type": "richtext",
      "required": true
    }
  }
}
