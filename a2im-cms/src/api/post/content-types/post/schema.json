{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "News Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Date": {
      "type": "datetime"
    },
    "Excerpt": {
      "type": "text"
    },
    "Slug": {
      "type": "string"
    },
    "Body": {
      "type": "richtext"
    },
    "app": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::app.app",
      "inversedBy": "news_posts"
    },
    "news_categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::news-category.news-category"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    }
  }
}
